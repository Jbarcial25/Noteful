  },
  "homepage": "https://github.com/a-vitug/react-app#readme",
  "dependencies": {
    "@apollo/client": "^3.6.6",
    "react-icons": "^4.3.1"
  },
  "devDependencies": {
    "concurrently": "^7.2.1"
  }
}